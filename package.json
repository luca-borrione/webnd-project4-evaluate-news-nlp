  },
  "scripts": {
    "check-yarn-integrity": "yarn check --integrity",
    "clean": "rm -rf dist",
    "compile": "webpack --config webpack.prod.js",
    "compile:dev": "webpack --config webpack.dev.js",
    "dev": "yarn clean && webpack-dev-server --config webpack.dev.js --open --port=3000",
    "format": "npm-run-all --parallel format:*",
    "format:css": "$npm_execpath run lint:css --fix",
    "format:html": "prettier --write 'public/**/*.html'",
    "format:js": "$npm_execpath run lint:js --fix",
    "format:json": "prettier --write '*.json'",
    "heroku-postbuild": "yarn compile",
    "lint": "npm-run-all --parallel lint:*",
    "lint:css": "stylelint 'public/**/*.css'",
    "lint:html": "linthtml 'public/**/*.html'",
    "lint:js": "eslint . --ext .js",
    "prepare": "husky install",
    "start": "node src/server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@linthtml/linthtml": "^0.8.0",
