    "eslint-plugin-import": "^2.23.4",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "prettier-package-json": "^2.6.0",
    "prettier-plugin-sort-json": "^0.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
